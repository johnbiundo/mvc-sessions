{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "arrow-parens": false,
    "interface-name": [
      false
    ],
    "max-line-length": [
      true,
      120
    ],
    "member-access": [
      false
    ],
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "quotemark": [
      true,
      "single"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
